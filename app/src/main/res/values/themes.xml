<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Sawii" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_blue2</item>
        <item name="colorPrimaryVariant">@color/dark_blue2</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">#535353</item>
        <item name="colorControlActivated">#FFC107</item>
    </style>

    <style name="MenuStyle" parent="ThemeOverlay.AppCompat.Dark">


        <item name="android:background">#FFFFFF</item>

    </style>

</resources>